import psycopg2

conn = psycopg2.connect(database = 'weather',
                        user = 'zakhar',
                        host = 'localhost',
                        port = 5432)
cur = conn.cursor()
name = "'Tula'"
cur.execute(f"SELECT * FROM weather WHERE city = {name};")
rows = cur.fetchall()

conn.commit()
cur.close()
conn.close()

for row in rows:
    name, login, passwd = row
    print(name)
